{
	"name": "iframe in new tab",
	"version": "1.5",
	"manifest_version": 3,
	"description": "Open an iframe in a new tab",
    "action": {
        "default_icon": "icon-128.png",
        "default_popup": "popup.html",
        "default_title": "iframe in new tab" 
    },
	"icons": {
        "16": "icon-16.png",
        "48": "icon-128.png"
	},
    "background": {
        "service_worker": "background.js"
    },
	"permissions": [
        "contextMenus"
    ]
}
